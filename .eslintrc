{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:@typescript-eslint/recommended"
  ],
  "rules": {
    "@typescript-eslint/interface-name-prefix": [
      "error", {
        "prefixWithI": "always"
      }
    ],
    "eslint no-extra-boolean-cast": 0,
    "no-use-before-define": ["error", { "functions": true, "classes": true }]
  }
}